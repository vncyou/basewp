{
    "name": "depchuanseo/wp",
    "type": "package",
    "keywords": [
        "WordPress",
        "Themes"
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.6|8.0|8.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "friendsofphp/php-cs-fixer": "3.*"
    },
    "scripts": {
        "cs": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcs",
        "cs:fix": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcbf",
        "cs:fixer": "@php ./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix"
    }
}
