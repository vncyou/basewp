{
    "extends": "standard",
    "rules": {
        "no-var": 0,
        "max-len": 0,
        "eqeqeq": 0,
        "no-sequences": 0,
        "new-cap": 0,
        "no-func-assign": 0,
        "no-cond-assign": 0,
        "camelcase": 0,
        "brace-style": 0,
        "no-self-assign": 0,
        "array-callback-return": 0,
        "no-unused-expressions": 0,
        "no-prototype-builtins": 0,
        "multiline-ternary": 0,
        "no-return-assign": 0,
        "no-void": 0,
        "no-undef": 0,
        "no-redeclare": 0,
        "no-mixed-spaces-and-tabs": 0,
        "no-tabs": 0,
        "no-useless-escape": 0,
        "no-mixed-operators": 0,
        "indent": [
            1,
            4
        ]
    }
}
